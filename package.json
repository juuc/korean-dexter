{
  "name": "korean-dexter",
  "version": "0.1.0",
  "description": "Autonomous AI financial research agent for Korean markets",
  "type": "module",
  "main": "src/index.tsx",
  "bin": {
    "korean-dexter": "./src/index.tsx"
  },
  "scripts": {
    "start": "bun run src/index.tsx",
    "dev": "bun --watch run src/index.tsx",
    "typecheck": "tsc --noEmit",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "docs:check": "markdown-link-check README.md CONTRIBUTING.md docs/*.md --config .markdown-link-check.json"
  },
  "dependencies": {
    "@langchain/anthropic": "^1.1.3",
    "@langchain/core": "^1.1.0",
    "@langchain/google-genai": "^2.0.0",
    "@langchain/openai": "^1.1.3",
    "dotenv": "^17.2.3",
    "fflate": "^0.8.2",
    "gray-matter": "^4.0.3",
    "ink": "^6.5.1",
    "ink-spinner": "^5.0.0",
    "ink-text-input": "^6.0.0",
    "langsmith": "^0.4.10",
    "react": "^19.2.0",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/react": "^19.2.7",
    "markdown-link-check": "^3.14.2",
    "typescript": "^5.9.3"
  }
}
